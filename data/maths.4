  9 (A4, 7, 8, 1) (a + b) + (c + 1) = a + (b + (c + 1))
]
10 (A1, 4, 9) ((a + b) + c = a + (b + c)) => ((a + b) + (c + 1) = a + (b + (c + 1)))
